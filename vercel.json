{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url",
      "NEXT_PUBLIC_URL": "https://kevincodes.vercel.app"
    },
    "build": {
      "env": {
        "DATABASE_URL": "@database_url"
      },
      "commands": [
        "npx prisma generate"
      ]
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  